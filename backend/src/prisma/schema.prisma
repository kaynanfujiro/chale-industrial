generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model aluguel {
  id          Int      @id(map: "Aluguel_pkey") @default(autoincrement())
  name        String
  description String
  imageP      String
  images      String[]
  video       String
  value       Int
  type        String
}

model useradmin {
  id       Int    @id(map: "UserAdmin_pkey") @default(autoincrement())
  username String
  password String
  email    String @unique(map: "UserAdmin_email_key")
}
